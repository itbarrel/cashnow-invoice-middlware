<%= turbo_frame_tag dom_id(document), class: "card shadow" do %>
    <div class="card-body card_style">
        <% if document.data.present? && (document.data.kind_of?(Hash) || document.data.kind_of?(Object)) %>
            <% document.data.keys.each do |key| %>
                <% if key.include?('invoice') %>
                    <%= key %> = <%= document.data[key] %><br>
                <% end %>
            <% end %>
        <% else %>
            No Data Found
        <% end %>
    </div>
<% end %>
