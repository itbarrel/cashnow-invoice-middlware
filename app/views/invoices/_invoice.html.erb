<div class="col-sm-4">
    <%= turbo_frame_tag dom_id(invoice) do %>
        <div class="card" style="width: 25rem;">
            <div class="card-body">
                <h5 class="card-title">Card title</h5>
                <% if invoice.data.present? && (invoice.data.kind_of?(Hash) || invoice.data.kind_of?(Object)) %>
                    <% invoice.data.keys.each do |key| %>
                        <% if key.include?('invoice') %>
                            <%= key %> = <%= invoice.data[key] %><br>
                        <% end %>
                    <% end %>
                <% end %>
            </div>
        </div>
    <% end %>
</div>


